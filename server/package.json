{
  "name": "photo-booth-backend",
  "version": "1.0.0",
  "description": "Photo storing app using GraphQL, Apollo Client, Node.js and React.js'",
  "scripts": {
    "server": "nodemon server.js",
    "client": "cd ../client && yarn start",
