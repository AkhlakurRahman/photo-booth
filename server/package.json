  },
  "main": "server.js",
  "dependencies": {
    "apollo-server": "^2.14.2",
    "dotenv": "^8.2.0",
    "graphql": "^14.6.0",
    "mongoose": "^5.8.11"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  },
  "license": "MIT"
}
